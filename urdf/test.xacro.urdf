<?xml version="1.0"?>
<robot name="robot" >

 <link name="base_link">
        

        
 </link>


  <link name="camera_link_optical">
  

  </link>


 <link name="body">
    
    <visual name="yellow">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="0.6 0.3 0.15"/>
        </geometry>
        <material name="white">
           
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <box size="0.6 0.3 0.15"/>
        </geometry>
    </collision>
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="5" />
        <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.15" iyz="0" izz="0.18" />
    </inertial>
    
 </link>

 <link name="wheel_left">
    
    <visual name="red">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.3" length="0.1"/>
        </geometry>
        <material name="">
            <color rgba="1.0 0.0 0.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.3" length="0.1"/>
        </geometry>
    </collision>

     <inertial>
      <mass value="2" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.048" ixy="0" ixz="0" iyy="0.48" iyz="0" izz="0.09" />
    </inertial>

 </link>

 <link name="wheel_right">
    
    <visual name="red">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.3" length="0.1"/>
        </geometry>
        <material name="fff">
            <color rgba="1.0 0.0 0.0 1.0"/>
            <texture filename=""/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
       <geometry>
            <cylinder radius="0.3" length="0.1"/>
        </geometry>
    </collision>
     <inertial>
      <mass value="2" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.048" ixy="0" ixz="0" iyy="0.48" iyz="0" izz="0.09" />
    </inertial>

 </link>

 <link name="wheel_caster">
    
    <visual name="red">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.1" length="0.1"/>
        </geometry>
        <material name="">
            <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.1" length="0.1"/>
        </geometry>
    </collision>
     <inertial>
      <mass value="3" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.048" ixy="0" ixz="0" iyy="0.48" iyz="0" izz="0.09" />
    </inertial>
 </link>

 <link name="camera_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
  </link>

  <joint name="camera_joint" type="fixed">
    
    <parent link="body"/>
    <child link="camera_link"/>
    <origin xyz="0.4 0 -0.3" rpy="0 0 0"/>

    <axis xyz="1 0 0" />
  </joint>


 <joint name="joint_base" type="fixed">
    <origin xyz="0.0 0.0 0.4" rpy="0.0 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="body"/>
    <axis xyz="0.0 0.0 0.0"/>
 </joint>

 <joint name="joint_left" type="continuous">
    <origin xyz="0.25 0.25 -0.1" rpy="-1.5708 0.0 0.0"/>
    <parent link="body"/>
    <child link="wheel_left"/>
    <axis xyz="0.0 0.0 1.0"/>

 </joint>

 <joint name="joint_right" type="continuous">
    <origin xyz="0.25 -0.25 -0.1" rpy="-1.5708 0.0 0.0"/>
    <parent link="body"/>
    <child link="wheel_right"/>
    <axis xyz="0.0 00.0 1.0"/>

 </joint>

 <joint name="joint_caster" type="fixed">
    <origin xyz="-0.4 0 -0.3" rpy="1.5708 0.0 0.0"/>
    <parent link="body"/>
    <child link="wheel_caster"/>
    <axis xyz="0.0 1.0 0.0"/> 

 </joint>



  <joint name="camera_optical_joint" type="fixed">
      <origin xyz="0 0 0" rpy="-1.57 0 -1.57" />
      <parent link="camera_link" />
      <child link="camera_link_optical" />
  </joint>

  <gazebo reference="camera_link">
      
      <sensor type="camera" name="camera1">
        <update_rate>30.0</update_rate>
        <camera name="head">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>800</width>
            <height>800</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>300</far>
          </clip>
          <noise>
            <type>gaussian</type>
            
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
          <updateRate>0.0</updateRate>
          <cameraName>rrbot/camera1</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>camera_link_optical</frameName>
          <hackBaseline>0.07</hackBaseline>
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
        </plugin>
      </sensor>
    </gazebo>




 <gazebo reference="base_link">
    <material>Gazebo/Orange</material>

    
  </gazebo>

  <gazebo reference="body">
    <material>Gazebo/Orange</material>
  </gazebo>

  <gazebo reference="weheel_left">
    <material>Gazebo/Orange</material>

   
  </gazebo>
  <gazebo reference="wheel_right">
    <material>Gazebo/Orange</material>
     
  </gazebo>

  <gazebo reference="body">
    <material>Gazebo/Orange</material>
  </gazebo>

  <gazebo reference="camera_link">
    <material>Gazebo/Orange</material>
  </gazebo>

  <gazebo reference="camera_link_optical">
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="wheel_caster">
    <material>Gazebo/Orange</material>
    <mu1 value="0.001"/>
    <mu2 value="0.001"/>
  </gazebo>

  

 

<gazebo>
 <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
       <robot_param>robot_description</robot_param>
      <robot_param_node>robot_state_publisher</robot_param_node>
      <parameters>$(find mypkg)/config/test_controllers.yaml</parameters>
    </plugin>
</gazebo>


<gazebo>
 <plugin name='diff_drive' filename='libgazebo_ros_diff_drive.so'>

        <ros>
          <namespace>/</namespace>
          <remapping>cmd_vel:=cmd_vel</remapping>
          <remapping>odom:=odom</remapping>
        </ros>

        <!-- wheels -->
        <left_joint>joint_left</left_joint>
        <right_joint>joint_right</right_joint>

        <!-- kinematics -->
        <wheel_separation>0.45</wheel_separation>
        <wheel_diameter>0.6</wheel_diameter>

        <!-- limits -->
        <max_wheel_torque>5</max_wheel_torque>
        <max_wheel_acceleration>1.0</max_wheel_acceleration>

        <!-- output -->
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <publish_wheel_tf>true</publish_wheel_tf>

        <odometry_frame>world</odometry_frame>
        <robot_base_frame>base_link</robot_base_frame>

      </plugin>
</gazebo>


  <gazebo>
        <plugin name="gazebo_ros_joint_state_publisher"
            filename="libgazebo_ros_joint_state_publisher.so">
            <update_rate>20</update_rate>
            <joint_name>joint_left</joint_name>
            <joint_name>joint_right</joint_name>
        </plugin>
    </gazebo>




</robot>